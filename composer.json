{
    "name": "ayles-software/laravel-mysql-s3-backup",
    "description": "Backup the MySQL database of your Laravel app to Amazon S3",
    "authors": [
        {
            "name": "Benjamin Ayles",
            "email": "ben@aylessoftware.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^9.0||^10.0||^11.0||^12.0",
        "aws/aws-sdk-php": "^3.63"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-0": {
            "LaravelMysqlS3Backup\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "LaravelMysqlS3Backup\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "cs-fix": "vendor/bin/php-cs-fixer fix"
    }
}
